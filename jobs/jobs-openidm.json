{
    "name": "OpenIDM repo shake out",
    "service-environment": "mac laptop hosting VirtualBox hosting Ubuntu 14.04 running OpenIDM 4.5.0",
    "service-location-port": "http://192.168.0.10:8080",
    "ADMINUSERID": "openidm-admin",
    "ADMINUSERPASSWORD": "openidm-admin",
    "load-generator": "mac laptop running JDK 8",
    "threads": 4,
    "threadgroupsize": 0,
    "threadinterval": 0,
    "upperbounds": 2000000,
    "workloads": [
        {
            "create-openidm-user-object": 1000
        },
        {
            "read-user-object": 0
        },
        {
            "update-user-object": 0
        },
        {
            "delete-user-object": 0
        },
        {
            "patch-user-object": 0
        },
        {
            "query-user-objects": 0
        }
    ]
}
